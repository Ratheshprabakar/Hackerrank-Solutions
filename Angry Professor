#include<stdio.h>
int main()
{
    int t;
    int n,k,i,j,count=0;
    int a[1000];
    scanf("%d",&t);
    for(i=0;i<t;i++)
{       count=0;
        scanf("%d %d",&n,&k);
        for(j=0;j<n;j++)
        {   
            scanf("%d",&a[j]);
            if(a[j]<=0)
            {
                count++;
            }
        }
        if(count<k)
        {
            printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
    }
}
