#include<stdio.h>
int main()
{
   int n,i,j,temp,x,count=0,max=0;
   int a[100000];
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
       if(max<a[i])
       {
           max=a[i];
       }
       
   }
   for(i=0;i<n;i++)
   {
       if(a[i]==max)
       {
           count++;
       }
   }
   printf("%d",count);
}
