
import math
import os
import random
import re
import sys

def solve(s): 
    temp=s.split(' ')
    return ' '.join((x.capitalize() for x in temp))

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    s = input()

    result = solve(s)

    fptr.write(result + '\n')

    fptr.close()
