#include <stdio.h>
int main() {
  int n;
  scanf("%d", &n);
  for (int i = 0; i<n;i++) {
    int x, y, z;
    scanf("%d %d %d", &x, &y, &z);
    if (abs(z - x) < abs(z - y))
      printf("Cat A\n");
    else if (abs(z - x) > abs(z - y))
      printf("Cat B\n");
    else
      printf("Mouse C\n");
  }
  return 0;
}
