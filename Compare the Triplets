#include<stdio.h>
int main()
{
    int a[100],b[100],sum1=0,sum2=0,i,j;
    for(i=0;i<3;i++)
    {
        scanf("%d\t",&a[i]);
    }
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0,j=0;i<3,j<3;i++,j++)
    {
        if(a[i]>b[j])
        {
            sum1++;
        }
        else if(b[j]>a[i])
              sum2++;
    }
    printf("%d %d",sum1,sum2);
    return 0;
}
