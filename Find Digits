def main():
    t=int(input())
    for i in range(t):
        count=0
        value=int(input())
        temp_list=[int(x) for x in str(value)]
        for j in temp_list:
            if j!=0:
                if(value%j==0):
                    count=count+1
        print(count)        
if __name__=='__main__':
    main()
