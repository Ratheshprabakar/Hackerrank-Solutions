def main():
    n=int(input())
    socks=[int(x) for x in input().split()]
    socks_count=0
    while socks !=[]:
        for i in socks:
            count_of_i=socks.count(i)
            socks_count=socks_count + (count_of_i//2)
            for j in range(0,count_of_i):
                socks.remove(i)
    print(socks_count)
if __name__=="__main__":
    main()
